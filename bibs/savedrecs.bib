@article{ ISI:000187435900001,
Author = {Doucet, F and Shukla, S and Otsuka, M and Gupta, R},
Title = {{BALBOA: A component-based design environment for system models}},
Journal = {{IEEE TRANSACTIONS ON COMPUTER-AIDED DESIGN OF INTEGRATED CIRCUITS AND
   SYSTEMS}},
