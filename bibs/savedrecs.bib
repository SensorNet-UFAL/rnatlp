@inproceedings{ ISI:000241630000010,
Author = {Bravenboer, Martin and de Groot, Rene and Visser, Eelco},
Editor = {{Lammel, R and Saraiva, J and Visser, J}},
Title = {{MetaBorg in action: Examples of domain-specific language embedding and
   assimilation using Stratego/XT}},
