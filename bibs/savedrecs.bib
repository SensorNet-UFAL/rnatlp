@inproceedings{ ISI:000181442400037,
Author = {Wang, YX and King, G},
Editor = {{Bellahsene, Z and Patel, D and Rolland, C}},
Title = {{A European COTS architecture with built-in tests}},
Booktitle = {{OBJECT-ORIENTED INFORMATION SYSTEMS, PROCEEDINGS}},
