Title = {{Analysis and Modelling of Power-Dependent Harmonic Characteristics of
   Modern PE Devices in LV Networks}},
Journal = {{IEEE TRANSACTIONS ON POWER DELIVERY}},
Year = {{2017}},
Volume = {{32}},
