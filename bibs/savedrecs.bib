@article{ ISI:000375811900037,
Author = {Bharathi, Ajay Karthic B. Gopinath and Singh, Abhinav and Tucker, Conrad
   S. and Nembhard, Harriet B.},
Title = {{Knowledge discovery of game design features by mining user generated
   feedback}},
Journal = {{COMPUTERS IN HUMAN BEHAVIOR}},
Year = {{2016}},
Volume = {{60}},
Pages = {{361-371}},
Month = {{JUL}},
Abstract = {{The term ``Gamification{''} is an emerging paradigm that aims to employ
   game mechanics and game thinking to change behavior. Gamification offers
   several effective ways to motivate users into action such as challenges,
   levels and rewards. However, an open research problem is discovering the
   set of gamification features that consistently result in a higher
   probability of success for a given task, game or application. The
   objective of this paper is to bridge this knowledge gap by quantifying
   the gamification features that are consistently found in successful
   applications. Knowledge gained from this work will inform designers
   about the gamification features that lead to higher chances of an
   application's ``success, and the gamification features that do not
   significantly impact the success of an application. The case study
   presented in this work leverages demographic heterogeneity and scale of
   applications existing within mobile platforms to evaluate the impact of
   gamification features on the success or failure of those applications.
   The successful game design features identified have the potential to be
   embedded into interactive gamification platforms across various fields
   such as healthcare, education, military and marketing, in order to
   maintain or enhance user engagement. (C) 2016 Elsevier Ltd. All rights
   reserved.}},
