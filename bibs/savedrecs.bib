@article{ ISI:000360591600003,
Author = {Bodeveix, Jean-Paul and Filali, Mamoun and Garnacho, Manuel and
   Spadotti, Regis and Yang, Zhibin},
Title = {{Towards a verified transformation from AADL to the formal
   component-based language FIACRE}},
