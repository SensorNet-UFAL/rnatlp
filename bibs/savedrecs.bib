@article{ ISI:000083691100004,
Author = {Topping, CJ and Rehder, MJ and Mayoh, BH},
Title = {{Viola: A new visual programming language designed for the rapid
   development of interacting agent systems}},
Journal = {{ACTA BIOTHEORETICA}},
Year = {{1999}},
Volume = {{47}},
Number = {{2}},
Pages = {{129-140}},
Abstract = {{The construction of complex simulation models and the application of new
   computer hardware to ecological problems has resulted in the need for
   many ecologists to rely on computer programmers to develop their
   modelling software. However, this can lead to a lack of flexibility and
   understanding in model implementation and in resource problems for
   researchers. This paper presents a new programming language, Viola,
   based on a simple organisational concept which can be used by most
   researchers to develop complex simulations much more easily than could
   be achieved with standard programming languages such as C++. The
   language is object oriented and implemented through a visual interface.
   It is specifically designed to cope with complicated individual based
   behavioural simulations and comes with embedded concurrency handling
   abilities.}},
