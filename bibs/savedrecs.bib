@inproceedings{ ISI:000233748300040,
Author = {Chen, Y and Du, X and Zhou, XG and Peng, CL},
Editor = {{Shen, W and Lin, Z and Barthes, JPA and Li, T}},
Title = {{An automatic coverage analysis for SystemC using UML and aspect-oriented
   technology}},
