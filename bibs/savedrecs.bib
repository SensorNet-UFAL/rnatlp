@article{ ISI:000248534300012,
Author = {Arzen, Karl-Erik and Bicchi, Antonio and Dini, Gianluca and Hailes,
   Stephen and Johansson, Karl H. and Lygeros, John and Tzes, Anthony},
Title = {{A component-based approach to the design of networked control systems}},
