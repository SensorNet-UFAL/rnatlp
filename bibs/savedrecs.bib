@article{ ISI:000271211700023,
Author = {Pagano, Bruno and Andrieu, Olivier and Moniot, Thomas and Canou,
   Benjamin and Chailloux, Emmanuel and Wang, Philippe and Manoury, Pascal
   and Colaco, Jean-Louis},
Title = {{Experience Report: Using Objective Caml to Develop Safety-Critical
   Embedded Tools in a Certification Framework}},
Journal = {{ACM SIGPLAN NOTICES}},
Year = {{2009}},
Volume = {{44}},
Number = {{8-9}},
Pages = {{215-219}},
Month = {{AUG-SEP}},
Note = {{14th ACM SIGPLAN International Conference on Functional Programming,
   Edinburgh, SCOTLAND, AUG 31-SEP 02, 2009}},
Organization = {{ACM SIGPLAN}},
Abstract = {{High-level tools have become unavoidable in industrial software
   development processes. Safety-critical embedded programs don't escape
   this trend. In the context of safety-critical embedded systems, the
   development processes follow strict guidelines and requirements. The
   development quality assurance applies as much to the final embedded
   code, as to the tools themselves. The French company Esterel
