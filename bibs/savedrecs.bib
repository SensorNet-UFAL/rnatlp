@inproceedings{ ISI:000224947700025,
Author = {Hsiung, PA and Lin, SW},
Editor = {{Chin, WN}},
Title = {{Formal design and verification of real-time embedded software}},
Booktitle = {{PROGRAMMING LANGUAGES AND SYSTEMS, PROCEEDINGS}},
