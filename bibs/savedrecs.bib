@article{ ISI:000425561800033,
Author = {Vanolo, Alberto},
Title = {{Cities and the politics of gamification}},
Journal = {{CITIES}},
Year = {{2018}},
Volume = {{74}},
Pages = {{320-326}},
Month = {{APR}},
Abstract = {{Gamification is widely intended as the mobilisation and implementation
   of game elements in extra-ludic situations, including the management of
   social problems and issues. By mobilising virtual rewards and playful
   elements, mobile apps, websites, social initiatives and even urban
