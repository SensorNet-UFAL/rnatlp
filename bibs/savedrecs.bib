@article{ ISI:000318720600001,
Author = {Wang, Rui and Guan, Yong and Luo Liming and Li, Xiaojuan and Zhang, Jie},
Title = {{Component-Based Formal Modeling of PLC Systems}},
Journal = {{JOURNAL OF APPLIED MATHEMATICS}},
Year = {{2013}},
Abstract = {{Functional validation is an important task in complex embedded system.
   The formal modeling of PLC system for verification is a rough task. Good
   verification model should be faithful and concise. At one hand, the
   model must be consistent with the system at the other hand, the model
   must have suitable scale because of the state explosion problem of
   verification. This paper proposes a systemic method for the construction
   of verification model. PLC system architecture and PLC features are
   modeled as components. This is universal for all PLC applications. We
   give an automatic translation method for software modeling based on
   operational semantics. A small example is demonstrated for our approach.}},
