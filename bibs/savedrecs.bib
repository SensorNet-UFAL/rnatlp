@article{ ISI:A1995TH88200003,
Author = {HALANG, WA and KRAMER, B and TRYBUS, L},
Title = {{EXPLOITING A GRAPHICAL PROGRAMMING PARADIGM TO FACILITATE RIGOROUS
   VERIFICATION OF EMBEDDED SOFTWARE}},
