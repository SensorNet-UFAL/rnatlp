@article{ ISI:000305829600010,
Author = {Waechter, J. and Babeyko, A. and Fleischer, J. and Haener, R. and
   Hammitzsch, M. and Kloth, A. and Lendholt, M.},
Title = {{Development of tsunami early warning systems and future challenges}},
