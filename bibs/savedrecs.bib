@article{ ISI:000280077800001,
Author = {Assayad, Ismail and Yovine, Sergio},
Title = {{A scheduler synthesis methodology for joint SW/HW design exploration of
   SoC}},
Journal = {{DESIGN AUTOMATION FOR EMBEDDED SYSTEMS}},
Year = {{2010}},
Volume = {{14}},
Number = {{2}},
Pages = {{75-103}},
Month = {{JUN}},
Abstract = {{The introduction of high-performance applications such as multimedia
   applications into SoCs led the manufacturers to provide embedded SoCs
   able to offer an important computing power which makes it possible to
   answer the increasing requirements of future evolutions of these
   applications. One of the adopted solutions is the use of multiprocessor
   SoCs. In this paper, we present a joint SW/HW design exploration
   methodology for multiprocessor SoCs. The system model relies on
   transaction-level component-based models for modeling parallel software
   and multiprocessor hardware. Our proposal comprises two original points.
   First, we propose a composable software-level scheduler constraints
   synthesis technique. Second, we present a combined software-level and
   exploratory hardware-level schedulers. The methodology has the advantage
   of combining real-time requirements of software with effective
   exploitation of multiprocessor hardware. We describe and apply the
   methodology to synthesize a scheduler of a slice-based MPEG-4 video
   encoder on the multiprocessor Cake SoCs.}},
