@article{ ISI:000285973700001,
Author = {Tsai, Chun-Ming and Lee, Hsi-Jian},
Title = {{Efficiently extracting and classifying objects for analyzing color
   documents}},
Journal = {{MACHINE VISION AND APPLICATIONS}},
Year = {{2011}},
Volume = {{22}},
Number = {{1}},
Pages = {{1-19}},
Month = {{JAN}},
Abstract = {{Conventional objects extraction method are not efficient for color
   document image with large graphics. For example, the projection profile
   and connected component based methods scanning the large graphics many
   times. To display the large graphics are extracted, conventional methods
   use rectangle to represent it. Thus, scanning into the large graphics is
   time-consuming. In this paper, a novel system for efficiently analyzing
   color documents is proposed to solve above mentioned problem. The
   proposed system includes color transformation, background color
   determination, objects extraction by top-down method, and objects
   classification without parameters. The proposed color document analysis
   system is efficient because it scans only background pixels such that
   the temporal complexity is O (NB), where NB is the total number of
   background color pixels. Results of this study demonstrate that this
   system is more effective and efficient than other methods. Moreover, the
   proposed algorithm can be run in an embedded environment (such as a
   mobile device) and processed in real-time system due to its simplicity
   and efficiency.}},
