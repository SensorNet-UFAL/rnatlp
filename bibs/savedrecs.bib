@inproceedings{ ISI:000223635900002,
Author = {Hsiung, PA and Lin, SW},
Editor = {{Yang, LT and Guo, M and Gao, GR and Jha, NK}},
Title = {{Automatic synthesis and verification of real-time embedded software}},
